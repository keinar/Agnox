{
  "name": "agnox",
  "version": "3.3.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build:shared": "npm run build -w shared-types",
    "test": "npm run test --workspaces",
    "test:api": "vitest run --project api",
    "test:e2e": "playwright test -c tests/playwright.config.ts",
    "migration:dry-run": "ts-node migrations/001-add-organization-to-existing-data.ts --dry-run",
    "migration:run": "ts-node migrations/001-add-organization-to-existing-data.ts",
    "docs:start": "npm run start --prefix apps/docs",
    "docs:build": "npm run build --prefix apps/docs",
    "docs:sync": "claude -p \"Read .agents/doc-sync/SKILL.md and execute the DocSync workflow strictly following all steps including version bumping and changelog generation.\""
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^25.0.10",
    "bcrypt": "^6.0.0",
    "mongodb": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.13.4",
    "dotenv": "^17.2.3"
  }
}